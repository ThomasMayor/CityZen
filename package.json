  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build --prod",
    "ionic:serve": "ionic-app-scripts serve",
    "start:win": "tsc server.ts --outDir ./dist && SET NODE_ENV=development&& node ./dist/server.js",
    "start": "tsc server.ts --outDir ./dist && NODE_ENV=development && node ./dist/server.js",
    "start:dev": "npm run mongod & npm run server:dev & ionic serve --dev",
    "start:prod": "npm run server & ionic serve --prod",
    "start:devwin": "SET NODE_ENV=development&& START /B tsc server.ts -w --outDir ./dist & START /B nodemon ./dist/server.js",
    "test:win": "SET TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\"}&&SET NODE_ENV=test&& mocha -r ts-node/register ./test/**/*.test.ts",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' && NODE_ENV=test && mocha -r ts-node/register ./test/**/*.test.ts",
    "mongod": "mongod --dbpath /Users/info/Downloads/mongodb-osx-x86_64-3.4.6/data/db",
    "server:dev": "tsc server.ts -w --outDir ./dist & nodemon ./dist/server.js --dev --ignore src/ --ignore www/ --ignore .tmp/",
    "server": "tsc server.ts --outDir ./dist && NODE_ENV=prod node ./dist/server.js ",
    "deploy:server" : "tsc server.ts --outDir ./dist && cp ./server/package.json ./dist/package.json && cp ./server/.gitignore ./dist/.gitignore && git add --all && git commit -m 'upd version - `date`' && git add -f ./dist/ && git subtree split -P dist -b heroku-serve && git rm -r --cached ./dist/ && git add --all && git commit -m 'rm .temp folder' && git push heroku heroku-serve:master --force && git branch -D heroku-serve && heroku ps:scale web=1 && heroku open"
  },
